[master]
<% if @puppet4 -%>
    vardir = /opt/puppetlabs/server/data/puppetserver
    logdir = /var/log/puppetlabs/puppetserver
    rundir = /var/run/puppetlabs/puppetserver
    pidfile = /var/run/puppetlabs/puppetserver/puppetserver.pid
    codedir = /etc/puppetlabs/code
<% end -%>
<% if @certname -%>
    certname = <%= @certname %>
<% end -%>
<% if @dns_alt_names -%>
    dns_alt_names = <%= @dns_alt_names %>
<% end -%>
<% if @reports -%>
    reports = <%= @reports %>
<% end -%>
<% if @reporturl -%>
    reporturl = <%= @reporturl %>
<% end -%>
<% if @storeconfigs -%>
    storeconfigs = <%= @storeconfigs %>
<% end -%>
<% if @storeconfigs_backend -%>
    storeconfigs_backend = <%= @storeconfigs_backend %>
<% end -%>
<% if @dbadapter -%>
    dbadapter = <%= @dbadapter %>
<% end -%>
<% if @dbserver -%>
    dbserver = <%= @dbserver %>
<% end -%>
<% if @dbname -%>
    dbname = <%= @dbname %>
<% end -%>
<% if @dbuser -%>
    dbuser = <%= @dbuser %>
<% end -%>
<% if @dbpassword -%>
    dbpassword = <%= @dbpassword %>
<% end -%>
<% if @dbsocket -%>
    dbsocket = <%= @dbsocket %>
<% end -%>
<% @extraopts.sort_by {|key, value| key}.each do |key,value| -%>
    <%= key %> = <%= value %>
<% end -%>

